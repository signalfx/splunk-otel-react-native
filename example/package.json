{
  "name": "SplunkOtelReactNativeExample",
  "version": "1.1.5",
  "private": true,
  "scripts": {
    "android": "react-native run-android --appId com.splunkotelreactnativeexample.debug",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "pods": "pod-install --quiet",
    "devserver": "node __tests__/e2e/devserver/runDevServer.js",
    "build.ios.release": "react-native run-ios --mode Release",
    "build.release.install": "react-native run-android --variant=release",
    "build.release": "cd android && ./gradlew assembleRelease",
    "wdio.android.local": "wdio run __tests__/e2e/configs/wdio.android.local.conf.ts",
    "wdio.android.sauce": "wdio run __tests__/e2e/configs/wdio.android.sauce.conf.ts",
    "wdio.ios.local": "wdio run __tests__/e2e/configs/wdio.ios.local.conf.ts",
    "wdio.ios.sauce": "wdio run __tests__/e2e/configs/wdio.ios.sauce.conf.ts"
  },
  "dependencies": {
    "@opentelemetry/api": "^1.3.0",
    "@react-navigation/native": "^6.1.8",
    "@react-navigation/native-stack": "^6.9.14",
    "@splunk/otel-react-native": "file:..",
    "react": "18.2.0",
    "react-native": "0.72.5",
    "react-native-config": "^1.5.1",
    "react-native-safe-area-context": "^4.7.2",
    "react-native-screens": "^3.25.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/eslint-config": "^0.72.2",
    "@react-native/metro-config": "^0.72.11",
    "@tsconfig/react-native": "^3.0.2",
    "@types/jest": "^29.5.7",
    "@types/react": "^18.0.24",
    "@types/react-test-renderer": "^18.0.0",
    "@wdio/cli": "^8.20.5",
    "@wdio/local-runner": "^8.20.5",
    "@wdio/mocha-framework": "^8.20.3",
    "@wdio/sauce-service": "^8.20.5",
    "@wdio/spec-reporter": "^8.20.0",
    "babel-jest": "^29.7.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "^8.19.0",
    "express": "^4.18.2",
    "jest": "^29.7.0",
    "metro-react-native-babel-preset": "0.76.8",
    "prettier": "^2.4.1",
    "react-test-renderer": "18.2.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  }
